<h3>Ostatnie zarejstrowane firmy</h3>
<br />
<div class="row">
  <% allServiceProviders.slice(-4).reverse().forEach(provider => { %>

  <div class="col-sm-3">
    <h5><%= provider.globalname %></h5>
    <img
      src="<%=provider.photo%>"
      class="img-responsive"
      style="width: 100%"
      alt="Image"
    />
  </div>
  <% }); %>
</div>
